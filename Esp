--[[
  Script multifunção para Roblox executores externos (ex: Delta)
  - Interface de senha (expulsa se errar, animação se acertar, imagens)
  - Interface de velocidade ajustável (com foto)
  - Botão bônus: auto kick
  Troque os IDs das imagens para as suas preferidas!
]]

local allowedName = "testeescript" -- Coloque "" para qualquer nome
local correctPassword = "minhaSenhaSuperSecreta" -- Troque pela sua senha
local imageSenha = "rbxassetid://1246352333" -- Goku Decal (pode trocar)
local imageObrigado = "rbxassetid://1246352333" -- Goku Decal (pode trocar)
local imageVelocidade = "rbxassetid://1246352333" -- Goku Decal (pode trocar)

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

if not LocalPlayer then
    return print("Esse script só funciona em executores externos com LocalPlayer.")
end

if allowedName ~= "" and LocalPlayer.Name ~= allowedName then
    LocalPlayer:Kick("Você não tem permissão para usar este script.")
    return
end

-- Interface Senha
local guiSenha = Instance.new("ScreenGui")
guiSenha.Name = "SenhaGui"
guiSenha.Parent = game.CoreGui

local frameSenha = Instance.new("Frame", guiSenha)
frameSenha.Size = UDim2.new(0, 350, 0, 250)
frameSenha.Position = UDim2.new(0.5, -175, 0.5, -125)
frameSenha.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
frameSenha.BorderSizePixel = 0

local imgSenha = Instance.new("ImageLabel", frameSenha)
imgSenha.Size = UDim2.new(0, 80, 0, 80)
imgSenha.Position = UDim2.new(0.5, -40, 0, 10)
imgSenha.BackgroundTransparency = 1
imgSenha.Image = imageSenha

local titleSenha = Instance.new("TextLabel", frameSenha)
titleSenha.Size = UDim2.new(1, 0, 0, 40)
titleSenha.Position = UDim2.new(0, 0, 0, 100)
titleSenha.Text = "Digite a Senha de acesso"
titleSenha.TextSize = 22
titleSenha.BackgroundTransparency = 1
titleSenha.TextColor3 = Color3.new(1, 1, 1)
titleSenha.Font = Enum.Font.GothamBold

local textboxSenha = Instance.new("TextBox", frameSenha)
textboxSenha.Size = UDim2.new(0.8, 0, 0, 40)
textboxSenha.Position = UDim2.new(0.1, 0, 0, 150)
textboxSenha.PlaceholderText = "Digite a Senha aqui"
textboxSenha.TextSize = 18
textboxSenha.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
textboxSenha.TextColor3 = Color3.new(1, 1, 1)
textboxSenha.Font = Enum.Font.Gotham

local confirmSenha = Instance.new("TextButton", frameSenha)
confirmSenha.Size = UDim2.new(0.8, 0, 0, 36)
confirmSenha.Position = UDim2.new(0.1, 0, 0, 200)
confirmSenha.Text = "Confirmar"
confirmSenha.TextSize = 18
confirmSenha.BackgroundColor3 = Color3.fromRGB(30, 160, 30)
confirmSenha.TextColor3 = Color3.new(1, 1, 1)
confirmSenha.Font = Enum.Font.Gotham

local errorSenha = Instance.new("TextLabel", frameSenha)
errorSenha.Size = UDim2.new(1, 0, 0, 22)
errorSenha.Position = UDim2.new(0, 0, 0, 230)
errorSenha.BackgroundTransparency = 1
errorSenha.TextColor3 = Color3.fromRGB(255, 60, 60)
errorSenha.TextSize = 15
errorSenha.Text = ""
errorSenha.Font = Enum.Font.Gotham

-- Função animação obrigado
local function animacaoObrigado()
    frameSenha.Visible = false

    local guiObrigado = Instance.new("ScreenGui")
    guiObrigado.Name = "ObrigadoGui"
    guiObrigado.Parent = game.CoreGui

    local frame = Instance.new("Frame", guiObrigado)
    frame.Size = UDim2.new(1, 0, 1, 0)
    frame.Position = UDim2.new(0, 0, 0, 0)
    frame.BackgroundColor3 = Color3.new(0, 0, 0)
    frame.BackgroundTransparency = 1

    for i = 1, 10 do
        frame.BackgroundTransparency = 1 - (i * 0.1)
        wait(0.05)
    end

    local img = Instance.new("ImageLabel", frame)
    img.Size = UDim2.new(0, 120, 0, 120)
    img.Position = UDim2.new(0.5, -60, 0.4, 0)
    img.BackgroundTransparency = 1
    img.Image = imageObrigado

    local texto = Instance.new("TextLabel", frame)
    texto.Size = UDim2.new(1, 0, 0, 60)
    texto.Position = UDim2.new(0, 0, 0.7, 0)
    texto.Text = "Obrigado por acertar a senha!"
    texto.TextSize = 32
    texto.TextColor3 = Color3.new(1, 1, 1)
    texto.BackgroundTransparency = 1
    texto.Font = Enum.Font.GothamBold

    wait(2)
    guiObrigado:Destroy()
end

-- Interface de velocidade ajustável
local function abrirVelocidade()
    local guiVel = Instance.new("ScreenGui")
    guiVel.Name = "VelocidadeGui"
    guiVel.Parent = game.CoreGui

    local frameVel = Instance.new("Frame", guiVel)
    frameVel.Size = UDim2.new(0, 350, 0, 230)
    frameVel.Position = UDim2.new(0.5, -175, 0.5, -115)
    frameVel.BackgroundColor3 = Color3.fromRGB(38, 38, 38)
    frameVel.BorderSizePixel = 0

    local imgVel = Instance.new("ImageLabel", frameVel)
    imgVel.Size = UDim2.new(0, 80, 0, 80)
    imgVel.Position = UDim2.new(0.5, -40, 0, 12)
    imgVel.BackgroundTransparency = 1
    imgVel.Image = imageVelocidade

    local tituloVel = Instance.new("TextLabel", frameVel)
    tituloVel.Size = UDim2.new(1, 0, 0, 30)
    tituloVel.Position = UDim2.new(0, 0, 0, 100)
    tituloVel.Text = "Ajuste sua velocidade"
    tituloVel.TextSize = 22
    tituloVel.BackgroundTransparency = 1
    tituloVel.TextColor3 = Color3.new(1, 1, 1)
    tituloVel.Font = Enum.Font.GothamBold

    local slider = Instance.new("TextBox", frameVel)
    slider.Size = UDim2.new(0.8, 0, 0, 40)
    slider.Position = UDim2.new(0.1, 0, 0, 140)
    slider.PlaceholderText = "Digite a velocidade (padrão: 16)"
    slider.TextSize = 18
    slider.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
    slider.TextColor3 = Color3.new(1, 1, 1)
    slider.Font = Enum.Font.Gotham

    local btnSet = Instance.new("TextButton", frameVel)
    btnSet.Size = UDim2.new(0.8, 0, 0, 36)
    btnSet.Position = UDim2.new(0.1, 0, 0, 190)
    btnSet.Text = "Ajustar"
    btnSet.TextSize = 18
    btnSet.BackgroundColor3 = Color3.fromRGB(30, 160, 30)
    btnSet.TextColor3 = Color3.new(1, 1, 1)
    btnSet.Font = Enum.Font.Gotham

    btnSet.MouseButton1Click:Connect(function()
        local speed = tonumber(slider.Text)
        if speed and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
            LocalPlayer.Character.Humanoid.WalkSpeed = speed
            btnSet.Text = "Velocidade definida!"
        else
            btnSet.Text = "Valor inválido!"
        end
        wait(1)
        btnSet.Text = "Ajustar"
    end)
end

-- Botão bônus de auto kick
local function abrirAutoKick()
    local guiKick = Instance.new("ScreenGui")
    guiKick.Name = "KickGui"
    guiKick.Parent = game.CoreGui

    local frameKick = Instance.new("Frame", guiKick)
    frameKick.Size = UDim2.new(0, 300, 0, 100)
    frameKick.Position = UDim2.new(0.5, -150, 0.5, -50)
    frameKick.BackgroundColor3 = Color3.fromRGB(60, 20, 20)
    frameKick.BorderSizePixel = 0

    local btnKick = Instance.new("TextButton", frameKick)
    btnKick.Size = UDim2.new(0.8, 0, 0, 60)
    btnKick.Position = UDim2.new(0.1, 0, 0.2, 0)
    btnKick.Text = "Auto Kick (Clique para ser expulso)"
    btnKick.TextSize = 18
    btnKick.BackgroundColor3 = Color3.fromRGB(160, 30, 30)
    btnKick.TextColor3 = Color3.new(1, 1, 1)
    btnKick.Font = Enum.Font.GothamBold

    btnKick.MouseButton1Click:Connect(function()
        LocalPlayer:Kick("Expulso pelo Auto Kick!")
    end)
end

-- Checagem senha
confirmSenha.MouseButton1Click:Connect(function()
    if textboxSenha.Text == correctPassword then
        animacaoObrigado()
        wait(0.5)
        abrirVelocidade()
        abrirAutoKick()
    else
        errorSenha.Text = "Senha incorreta! Você será expulso..."
        wait(1)
        LocalPlayer:Kick("Senha incorreta!")
    end
end)
